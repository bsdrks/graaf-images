digraph {
    node [shape=circle, fontname="monospace"];
    edge [arrowsize=0.5, fontname="monospace"];
    layout=circo;
    pad=0.2;

    0 [label="0"];
    1 [label="1"];
    2 [label="2"];
    3 [label="3"];
    4 [label="4"];
    5 [label="5"];

    0 -> 4;
    1 -> 0;
    2 -> {1, 3, 5};
    5 -> 4;
}