digraph {
    node [shape=circle, fontname="monospace", style=bold, fixedsize=true, width="0.5"];
    edge [arrowsize=0.6, fontname="monospace", fontsize=9];
    layout=neato;
    mode="sgd";
    maxiter=200000;
    normalize=true;
    scale=1.0;
    pad=0.2;

    0 -> 1 [dir="both"];
    0 -> 2;
    1 -> 3;
    1 -> 4 [dir="both"];
    3 -> 0;
    3 -> 2;
}