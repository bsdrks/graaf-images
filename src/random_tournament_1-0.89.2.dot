digraph {
    node [shape=circle, fontname=monospace; fixedsize=true, width=0.5];
    edge [arrowsize=0.6];
    color=black;
    fontcolor=black;
    bgcolor=white;
    layout=neato;
    scale=1.5;
    mode=sgd;
    maxiter=10000;
    normalize=true;
    pad=0.2;

    0 -> 5;
    1 -> 0;
    1 -> 4;
    1 -> 5;
    2 -> 0;
    2 -> 1;
    2 -> 3;
    2 -> 5;
    3 -> 0;
    3 -> 1;
    3 -> 5;
    4 -> 0;
    4 -> 2;
    4 -> 3;
    5 -> 4;
}