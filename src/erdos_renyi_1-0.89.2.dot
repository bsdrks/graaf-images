digraph {
    node [shape=circle, fontname=monospace; fixedsize=true, width=0.5];
    edge [arrowsize=0.6];
    color=black;
    fontcolor=black;
    bgcolor=white;
    layout=neato;
    scale=1.2;
    mode=sgd;
    maxiter=10000;
    normalize=true;
    pad=0.2;

    0 -> 3 [dir=both];
    0 -> 4 [dir=both];
    1 -> 2 [dir=both];
    1 -> 5;
    2 -> 0;
    2 -> 3 [dir=both];
    2 -> 5;
    3 -> 5;
    4 -> 1;
    4 -> 2;
    5 -> 0;
}