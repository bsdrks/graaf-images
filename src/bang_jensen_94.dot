digraph {
    node [shape=circle, fontname=monospace];
    edge [arrowsize=0.6];
    color=black;
    fontcolor=black;
    bgcolor=white;
    layout=neato;
    mode=sgd;
    normalize=true;
    scale=1.0;
    maxiter=100000;
    pad=0.2;

    0 -> {1, 2};
    1 -> {3};
    2 -> {1, 3, 4, 5};
    3 -> {5};
    4 -> {6};
}