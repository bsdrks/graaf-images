digraph {
    node [shape=circle, fontname="Operator Mono"];
    edge [arrowsize=0.5];
    layout=circo;
    pad=0.2;

    subgraph cluster_0 {
        style="dashed";
        0 [label="0"];
        1 [label="1"];
    }

    subgraph cluster_1 {
        style="dashed";
        2 [label="2"];
        3 [label="3"];
        4 [label="4"];
    }

    0 -> 2 [dir="both"];
    1 -> 2 [dir="both"];
    0 -> 3 [dir="both"];
    1 -> 3 [dir="both"];
    0 -> 4 [dir="both"];
    1 -> 4 [dir="both"];
}