digraph {
    node [shape=circle, fontname=monospace, fixedsize=true, width=0.5];
    edge [arrowsize=0.6];
    color=black;
    fontcolor=black;
    bgcolor=white;
    layout=neato;
    mode=sgd;
    normalize=true;
    maxiter=100000;
    pad=0.2;

    0 -> {1} [dir=both];
    1 -> {7};
    2 -> {0, 5};
    2 -> {7} [dir=both];
    3 -> {4} [dir=both];
    5 -> {3};
    5 -> {6} [dir=both];
    8 -> {7, 11};
    8 -> {9} [dir=both];
    10 -> {9};
    10 -> {11} [dir=both];
}