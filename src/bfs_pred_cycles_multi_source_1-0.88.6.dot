    0 -> 5 [style=invis];
    1 -> 2 [color=lightsalmon; style=bold];
    1 -> 3 [style=invis];
    2 -> 0 [color=lightsalmon; style=bold];
    3 -> 4 [color=palegreen3; style=bold];
