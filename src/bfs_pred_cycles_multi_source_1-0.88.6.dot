digraph {
    node [shape=circle; style=bold; fontname="monospace"; fixedsize=true; width="0.75"];
    edge [arrowsize=0.6; fontname="monospace"; fontsize=9];
    layout=neato;
    mode="sgd";
    scale=1.2;
    maxiter=100000;
    normalize=true;
    pad=0.2;
    splines=line

    0 [
        label=<
            <table border="0" cellpadding="1" cellspacing="0">
                <tr>
                    <td>0</td>
                </tr>
                <tr>
                    <td><font point-size="9">c(0)=0</font></td>
                </tr>
            </table>
        >;
        color=lightsalmon;
    ];

