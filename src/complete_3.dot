digraph {
    node [shape=circle, fontname=monospace; fixedsize=true, width=0.5];
    edge [arrowsize=0.6];
    color=black;
    fontcolor=black;
    bgcolor=white;
    layout=neato;
    mode=sgd;
    normalize=true;
    maxiter=10000;
    pad=0.2;
    scale=1.0;

    0 -> {1, 2} [dir=both];
    1 -> 2 [dir=both];
}