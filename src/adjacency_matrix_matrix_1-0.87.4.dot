    7, 14, 21, 28, 35 [style=dashed, color="#aaa", fontcolor=white];

    edge [style=invis];

    0 -> 6 -> 12 -> 18 -> 24 -> 30;
    1 -> 7 -> 13 -> 19 -> 25 -> 31;
    2 -> 8 -> 14 -> 20 -> 26 -> 32;
    3 -> 9 -> 15 -> 21 -> 27 -> 33;
    4 -> 10 -> 16 -> 22 -> 28 -> 34;
    5 -> 11 -> 17 -> 23 -> 29 -> 35;

    rank=same {0; 1; 2; 3; 4; 5};
