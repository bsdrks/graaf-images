digraph {
    node [shape=circle, fontname=monospace];
    edge [arrowsize=0.6, fontname=monospace];
    color=black;
    fontcolor=black;
    bgcolor=white;
    layout=neato;
    mode=sgd;
    maxiter=10000;
    normalize=true;
    pad=0.2;

    0 -> {3, 4};
    2 -> {3, 4, 5};
    3 -> {1};
    4 -> {1};
    5 -> {1};
}