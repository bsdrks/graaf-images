digraph {
    node [shape=circle, fontname="monospace", fixedsize=true, width=0.5, height=0.5];
    edge [arrowsize=0.6, fontname="monospace"];
    pad=0.2;
    nodesep=0.4;
    ranksep=0.4;

    1 -> {2, 5} [dir=both];
    2 -> {6} [dir=both];
    5 -> {6, 9} [dir=both];
    12 -> {13} [dir=both];
    13 -> {9} [dir=both];

    edge [style=invis];

    0 -> 4 -> 8 -> 12;
    6 -> 10 -> 14;
    3 -> 7 -> 11 -> 15;

    rank=same {0; 1; 2; 3};
    rank=same {4; 5; 6; 7};
    rank=same {8; 9; 10; 11};
    rank=same {12; 13; 14; 15};
}