digraph {
    node [shape=circle, fontname="monospace"];
    edge [arrowsize=0.6, fontname="monospace", fontsize=9];
    layout=circo;
    scale=1.0;
    normalize=true;
    pad=0.2;
    splines=line

    3 [color="#aaaaaa", fontcolor="#aaaaaa"];
    0, 1, 2, 4, 5 [style=bold, color=red];

    0 -> 1 [style=bold, color=red, headlabel="t=1  ", taillabel="t=0  "];
    3 -> 0 [color="#aaaaaa"];
    1 -> 2 [style=bold, color=red, headlabel="   t=2"];
    2 -> 3 [style=invis];
    1 -> 4 [style=bold, color=red, headlabel="t=4   "];
    2 -> 5 [style=bold, color=red, headlabel="   t=3 "];
}