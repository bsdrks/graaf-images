digraph {
    node [shape=circle, fontname="Operator Mono"];
    edge [arrowsize=0.5];
    layout=circo;
    pad=0.2;

    0 [label="0"];
    1 [label="1"];
    2 [label="2"];
    3 [label="3"];
    4 [label="4"];

    0 -> 2;
    0 -> 3;
    0 -> 4;
    1 -> 2;
    1 -> 3;
    1 -> 4;
    2 -> 0;
    2 -> 1;
    3 -> 0;
    3 -> 1;
    4 -> 0;
    4 -> 1;
}