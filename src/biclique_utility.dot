digraph {
    node [shape=circle, fontname="monospace"; fixedsize=true, width="0.5"];
    edge [arrowsize=0.6];
    layout=circo;
    normalize=true;
    scale=0.8;  
    pad=0.2;

    subgraph cluster_0 {
        0, 1, 2;
    }

    subgraph cluster_1 {
        3, 4, 5;
    }

    0 -> {3, 4, 5} [dir="both"];
    1 -> {3, 4, 5} [dir="both"];
    2 -> {3, 4, 5} [dir="both"];
}