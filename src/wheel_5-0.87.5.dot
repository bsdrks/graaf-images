digraph {
    node [shape=circle; fontname=monospace; fixedsize=true; width=0.5];
    edge [arrowsize=0.6];
    color=black;
    fontcolor=black;
    bgcolor=white;
    layout=neato;
    mode=sgd;
    maxiter=10000;
    normalize=true;
    scale=1.2;
    pad=0.2;

    0 -> 1 [dir=both];
    0 -> 2 [dir=both];
    0 -> 3 [dir=both];
    0 -> 4 [dir=both];
    4 -> 1 [dir=both];
    1 -> 2 [dir=both];
    2 -> 3 [dir=both];
    3 -> 4 [dir=both];
}