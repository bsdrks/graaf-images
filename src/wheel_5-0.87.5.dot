digraph {
    node [shape=circle, fontname="monospace"; fixedsize=true, width="0.5"];
    edge [arrowsize=0.6];
    layout=neato;
    scale=1.3;
    mode="sgd";
    normalize=true;
    pad=0.2;

    0 -> 1 [dir="both"];
    0 -> 2 [dir="both"];
    0 -> 3 [dir="both"];
    0 -> 4 [dir="both"];
    4 -> 1 [dir="both"];
    1 -> 2 [dir="both"];
    2 -> 3 [dir="both"];
    3 -> 4 [dir="both"];
}